<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pku.pojo.UserSystem" table="t_user_system">
		<id name="id" type="integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="userSysName" type="string">
			<column name="userSysName" length="200" not-null="true" />
		</property>
		<property name="organization" type="string">
			<column name="organization" length="200" />
		</property>
		<property name="phyAddr" type="string">
			<column name="phyAddr" length="200" />
		</property>
		<property name="ipAddr" type="string">
			<column name="ipAddr" length="100" />
		</property>
		<property name="upperDataAccessNode" type="string">
			<column name="upperDataAccessNode" length="200" not-null="true" />
		</property>
		<property name="sysType" type="string">
			<column name="sysType" length="200" />
		</property>
		<property name="state" type="string">
			<column name="state" length="100" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="createTime" length="0">
			</column>
		</property>
				<property name="comment" type="string">
			<column name="uComment" length="500">
			</column>
		</property>
		</class>
</hibernate-mapping>