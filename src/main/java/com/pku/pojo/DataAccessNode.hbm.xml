<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pku.pojo.DataAccessNode" table="t_data_access_node">
		<id name="id" type="integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="accessNodeName" type="string">
			<column name="accessNodeName" length="200" not-null="true" />
		</property>
		<property name="accessNodeCapacity" type="string">
			<column name="accessNodeCapacity" length="200" />
		</property>
		<property name="accessNodeKind" type="string">
			<column name="accessNodeKind" length="200" />
		</property>
		<property name="ipAddr" type="string">
			<column name="ipAddr" length="100" />
		</property>
		<property name="upperDataCenter" type="string">
			<column name="upperDataCenter" length="100" not-null="true" />
		</property>
		<property name="accessNodeServiceCapacity" type="string">
			<column name="accessNodeServiceCapacity" length="200" />
		</property>
		<property name="priority" type="string">
			<column name="priority" length="200" />
		</property>
		<property name="state" type="java.lang.String">
			<column name="state" length="50">
			</column>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="createTime" length="0">
			</column>
		</property>
				<property name="phyAddr" type="string">
			<column name="phyAddr" length="100" not-null="true"/>
			</property>
					<property name="comment" type="string">
			<column name="aComment" length="500">
			</column>
		</property>
	</class>
</hibernate-mapping>